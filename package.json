{
  "name": "electron-screen-recorder",
  "executableName": "electron-screen-recorder",
  "productName": "electron-screen-recorder",
  "version": "1.0.1",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "electron-forge start",
